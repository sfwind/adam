<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="oauth" namespace="/oauth" extends="struts-default">
        <action name="oauthCode"
                class="com.dianping.ba.es.qyweixin.adapter.web.action.OAuthAction"
                method="oauthCode">
        </action>

        <action name="authenticate"
                class="com.dianping.ba.es.qyweixin.adapter.web.action.OAuthAction"
                method="authenticate">
        </action>

        <action name="userId"
                class="com.dianping.ba.es.qyweixin.adapter.web.action.OAuthAction"
                method="userId">
        </action>
    </package>

</struts>